{
  "name": "twitch-healthbar-overlay",
  "version": "1.0.0",
  "description": "OBS healthbar overlay controlled by Twitch emote/redeem events",
  "main": "server.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\" \"npm run client\"",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "multer": "^1.4.5-lts.1",
    "websocket": "^1.0.34",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.6.5",
    "@types/websocket": "^1.0.7",
    "@types/ws": "^8.5.6",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "prisma": "^5.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
